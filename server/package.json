    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.29.1",
    "postgres": "^3.4.3",
    "@neondatabase/serverless": "^0.7.2",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "aws-sdk": "^2.1506.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
